{
  "name": "memview",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,tsx --exec npm run fast-build",
    "build": "npm run clean-dist && npm run clean-temp && npm run build-back && npm run build-front && npm run temp-to-dist && npm run copy-package && npm run copy-readme && npm run copy-license",
    "fast-build": "npm run build-back && npm run build-front && npm run temp-to-dist",
    "build-back": "rollup -c",
    "build-front": "cd ./src/front/ && npm run build && cd ./dist && cpy . ../../../temp/",
    "temp-to-dist": "cd ./temp && cpy . ../dist/",
    "clean-dist": "rimraf dist",
    "clean-temp": "rimraf temp",
    "copy-package": "cd assets && cpy package.json ../dist",
    "copy-readme": "cd .. && cpy README.md ./package/dist",
    "copy-license": "cd .. && cpy LICENSE ./package/dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.0",
    "@types/express": "^5.0.0",
    "@types/lodash": "^4.17.9",
    "@types/openurl": "^1.0.3",
    "@types/socket.io": "^3.0.2",
    "babel-loader": "^9.2.1",
    "cpy-cli": "^5.0.0",
    "nodemon": "^3.1.7",
    "rimraf": "^6.0.1",
    "rollup": "^4.23.0",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-loader": "^9.5.1",
    "tslib": "^2.7.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "express": "^4.21.1",
    "openurl": "^1.1.1",
    "socket.io": "^4.8.0"
  }
}
